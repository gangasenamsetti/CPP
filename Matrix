//input and output in matrix
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int mat[n][n];
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            cin>>mat[i][j];
        }
    }
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            cout<<mat[i][j]<<" ";//In row wise
            cout<<mat[j][i]<<" "; //In column wise
        }
        cout<<endl;
    }
}



//Matrix diagonals sum
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int mat[n][n];
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            cin>>mat[i][j];
        }
    }
    int s=0;
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            if(i==j) s+=mat[i][j];
            if(i+j==n-1) s+=mat[i][j]; //i+j==n-1 means we get 02 and 20 elements
        }
        
    }
    cout<<s;
}
