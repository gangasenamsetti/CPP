//to get individual characters in a sentence of string

#include <bits/stdc++.h>
using namespace std;
int main() {
    // Write C++ code here
   string s;
   getline(cin,s); //this is used to read entire line of string
   stringstream ss(s);
   vector<string>v;
   string word;
   while(ss>>word){
       v.push_back(word);
   }
   for(auto it:v) cout<<it<<" ";

    return 0;
}
