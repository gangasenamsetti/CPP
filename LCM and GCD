//GCD of two numbers-->greatest common factor   1st method
#include <bits/stdc++.h>
using namespace std;
int main() {
    int a=60,b=36,ans=0;
    int res=min(a,b);
    while(res>0){
        if(a%res==0 and b%res==0){
            ans=res;
            break;
        }
        res--;
    }
    cout<<ans;

    return 0;
}


//2nd method--->recursive method of finding GCD
#include <bits/stdc++.h>
using namespace std;
int gcd(int a,int b){
    if(a==0) return b;
    if(b==0) return a;
    if(a==b) return a;
    if(a>b){
        return gcd(a-b,b);
    }
    return gcd(a,b-a);
}
int main() {
    int a,b;
    cin>>a>>b;
    cout<<gcd(a,b);

    return 0;
}
