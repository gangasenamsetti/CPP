#include <iostream>
using namespace std;

class Node{
    public:
    int data;
    Node *next;
    Node(int val){
        this->data=val;
        this->next=NULL;
    }
};

class LinkedList
{
    Node *head,*tail;
    public:
    LinkedList(){
        head=tail=NULL;
    }
    void insert(int val)
    {
        Node *nn=new Node(val);
        if(head==NULL){
            head=nn;
            tail=nn;
        }
        else{
            tail->next=nn;
            tail=nn;
        }
    }
    void display()
    {
        Node *temp=head;
        while(temp){
            cout<<temp->data<<" ";
            temp=temp->next;
        }
    }
};
int main()
{
   
    LinkedList list;
    list.insert(10);
    list.insert(20);
    list.display();
    
    
}


