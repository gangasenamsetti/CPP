 #include <bits/stdc++.h>
using namespace std;
int main() {
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++) cin>>arr[i];
    int pref[n]={0};
    pref[0]=arr[0];
    for(int i=1;i<n;i++){
        pref[i]=pref[i-1]+arr[i];
    }
    for(int i=0;i<n;i++){
        cout<<pref[i]<<" ";
    }

    return 0;
}

o/p:
5
1 2 3 4 5
1 3 6 10 15 

